<%- include ('../partials/header') %>
<div class="container">
	<div style="margin:100px auto">
		
<h1 class="text-center">
	Create new Campground
</h1>
	<div style="width:40%;margin:25px auto">
		
<form action="/campgrounds" method="post">
	<div class="form-group">
		<input type="text" placeholder="name" name="name" class="form-control">
	</div>	
	<div class="form-group">
		<input type="text" placeholder="image url" name="image" class="form-control">
	</div>	
	
	<div class="form-group">
		<input type="text" placeholder="description" name="description" class="form-control">
	</div>	
	<div class="form-group">
		<input type="number" placeholder="price" name="price" class="form-control" min=".01" step=".01">
	</div>
	
	<div class="form-group">
		<input class="btn btn-block btn-primary" type="submit" value="submit">
	</div>	
	
	
	
		
	
</form>
<a class="btn btn-success" href="/campgrounds">Go Back</a>
	</div>
	</div>
</div>

<%- include ('../partials/footer') %>